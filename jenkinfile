node {
      stage('Git clone') { 
      git branch: 'main', credentialsId: 'github2', url: 'https://github.com/harishvani/jenkinsfile.git'
    }
      
	  stage('maven clean') { 
        sh 'mvn clean'
    }
	 
	  stage('maven validate') {
	     sh 'mvn validate'
	}
    
      stage('maven compile') {
        sh 'mvn compile'
    }
    
      stage('maven Test') {
        sh 'mvn test'
    }  

      stage('maven Package') {
        sh 'mvn package'	
    
    }	
	  
    	 
 } 
 
 
